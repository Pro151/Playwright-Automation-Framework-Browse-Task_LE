{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:playwright/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["playwright"],
    "rules": {
      // Add any custom rules here
      "no-unused-vars": "warn",
      "semi": ["error", "always"]
    }
  }
  